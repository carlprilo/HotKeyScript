
^right::
    send, {lwin down}{ctrl down}{right}
    sleep 50
    send, {lwin up}{ctrl up}
return


^left::
    send, {lwin down}{ctrl down}{left}
    sleep 50
    send, {lwin up}{ctrl up}
return

!space::
	send, {ctrl down}
	sleep 10
	send, {ctrl up}
	sleep 10
	send, {ctrl down}
	sleep 10
	send, {ctrl up}
return

